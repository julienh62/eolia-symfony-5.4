{% extends 'base.html.twig' %}

{% block title %}admin-categorie{% endblock %}

{% block body %}

<main class="container">
   <h1>Créer une catégorie: </h1>
   


{#} {{ form(formView) }}#}

{{ form_start(formView) }}

{{ form_errors(formView) }}

<div class="row">
  <div class="col">
     {{ form_row(formView.title) }}
    
 </div>
  <div class="col">
     {{ form_row(formView.slug) }} 

  </div>
</div>

{#{{ form_widget(formView) }} #}

 <button type="submit" class="btn btn-primary">
 <i class="fas fa-save"> </i>
         Créer une catégorie
 </button>
{{ form_end(formView) }}
</main>
{% endblock %}